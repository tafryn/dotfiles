!!! Xaw Scrollbars
*Scrollbar.foreground: 	dark slate blue
*Scrollbar.background: 	black

!!! Xaw Menus
*SimpleMenu*background: rgb:10/10/10
*SimpleMenu*foreground: rgb:20/20/a0

*cursorColor: 			SeaGreen
!*pointerColor: rgb:40/50/c0

!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
! emacs

emacs*XlwMenu.foreground: rgb:33/99/ff
Emacs*Background: 		rgb:00/00/03
Emacs*Foreground: 		ivory3
emacs*bitmapIcon: 		on
emacs*pointerColor: 	forestGreen
emacs*font: 			fixed


!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
! xdvi

xdvi.foreground: 		black
xdvi.background: 		white

!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
! XTerm

xterm*pointerColorBackground: rgb:40/50/c0
*VT100.color3: 			light goldenrod
xterm*scrollBar: 		true
xterm*Scrollbar.foreground: dark slate blue
xterm*faceName: DejaVu Sans Mono for Powerline
xterm*renderFont: true
xterm*faceSize: 12

! Number of Lines to buffer
xterm*saveLines: 		2048

!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
! urxvt
!URxvt.font: -*-bitstream vera sans mono-*-r-*-*-14-*-*-*-*-*-*-*
URxvt*font: 			xft:DejaVu Sans Mono for Powerline:size=10
!URxvt.font: 			xft:Bitstream Vera Sans Mono:size=8
!URxvt.boldfont: 		xft:Go Mono for Powerline:bold:size=10
!URxvt.italicfont: 		xft:Go Mono for Powerline:italic:size=10
!URxvt.bolditalicfont: 	xft:Go Mono for Powerline:bold:italic:size=10
URxvt.shading:			20
URxvt.transparent:		true
URxvt.scrollBar: 		false
URxvt.urgentOnBell:		true
URxvt.secondaryScroll:	true
URxvt.saveLines: 		4096
URxvt.perl-ext-common:	default,matcher
URxvt.url-launcher:		/usr/bin/firefox
URxvt.matcher.button:	1
URxvt.underlineColor:	#ff87ff
! These two lines compensate for urxvts crappy control codes
URxvt.keysym.C-Prior:	[5;5~
URxvt.keysym.C-Next:	[6;5~
! Color palette
URxvt.foreground:		#ffffff
URxvt.background:		#0e0e0e

spellcast*name:			Me
